/***************** menu *********************/
.context-menu {
    position:absolute;
    overflow: hidden;
    z-index:200;
    padding:5px 0;
    width:160px;
    display: none;
    background-color:#FFF;
    border: 1px solid #c8ccd5;
    box-shadow: 0px 1px 3px rgba(0,0,0,0.2);
    color:#616161;
}
.context-menu .text {
    padding:0 30px;
    display:block;
    text-decoration:none;
    color:#616161;
    line-height:32px;
}
.context-menu .text:hover {
    background-color:#efefef;
}
.context-menu .xian {
    border-bottom:#e5e5e5 solid 1px;
    background-color:#FFF;
    margin:3px 0;
    height: 0px;
}
.context-menu .ie7height {
    +height:24px;
}
.context-menu .menubar .sanjiao {
    background-position:0 -390px;
    position:absolute;
    width:10px;
    height:9px;
    top:7px;
    right:15px;
    *top:12px;
}
.context-menu .sub-menu .text:hover {
    background-color:#efefef;
}
.context-menu .menubar {
    position:relative;
    position:static;
    zoom:1;
}
.context-menu .sub-menu {
    position:absolute;
    left:140px;
    top:-6px;
    padding-left:3px;
    background:transparent;
    height:100px;
    display:none;
}
.context-menu .sub-left {
    left:auto;
    right:170px;
    padding-right:5px;
}
.context-menu .menubar:hover .sub-menu {
    display:block;
}
