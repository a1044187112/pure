@charset "utf-8";

html,body,div,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,p,blockquote,pre,form,input,textarea,fieldset,table,th,td,header,footer,nav,article,section {
	margin: 0;
	padding: 0
}

body,button,input,select,textarea {
	font: 12px/1.5 tahoma,arial,宋体
}

body {
	background: #fff;
	color: #333
}

ul,ol {
	list-style: none
}

img {
	border: none;
	vertical-align: top
}

header,footer,nav,article,section {
	display: block
}

h1 {
	font-size: 18px
}

h2 {
	font-size: 16px
}

h3 {
	font-size: 14px
}

h4,h5,h6 {
	font-size: 100%
}

a {
	color: #0068b7;
	outline: none
}

a,a:hover {
	text-decoration: none
}

table {
	border-collapse: collapse;
	border-spacing: 0
}

:focus {
	outline: 0
}

.f_l {
	float: left
}

.f_r {
	float: right
}

.c_b,.clear {
	clear: both
}

.clear {
	font-size: 0;
	height: 0;
	overflow: hidden
}

.clearfix {
	zoom: 1
}

.f_w_b {
	font-weight: 700
}

.f_w_n {
	font-weight: 400
}

.black {
	color: #000
}

.red {
	color: red
}

.gray {
	color: #999
}

.white {
	color: #fff
}

.mt10{
	margin-top: 10px;
}

.mt20{
	margin-top: 20px;
}

.mt30{
	margin-top: 30px;
}

.error{
	color: #FF0000;
}

@media (min-width: 768px) {
  .mts {
    margin-top: 20px;
  }
  .mbs {
    margin-bottom: 20px;
  }
  .mtm {
    margin-top: 40px;
  }
  .mbm {
    margin-bottom: 40px;
  }
  .mtl {
    margin-top: 60px;
  }
  .mbl {
    margin-bottom: 60px;
  }
  .mtx {
    margin-top: 100px;
  }
  .mbx {
    margin-bottom: 100px;
  }
}


.navbar-static-top {
  background: transparent;
  margin: 0px;
}

.navbar-logo {
  /*background-image:url(../img/logo-cn.png);*/
  background-position: center;
  background-repeat: no-repeat;
  float: left;
  height: 68px;
  line-height: 68px;
  width: 180px;
  font-size: 32px;
  font-weight: bold;
  color: #0068B7;
  font-family: Helvetica, "Helvetica Neue", "Century Gothic", "微软雅黑", "Microsoft Yahei", "STXihei";
}

.navbar-logo span{
	color: #666;
	font-size: 14px;
}

.navbar .navbar-nav > li > a {
  /*color: #f45531;*/
 height: 38px;
 line-height: 38px;
  font-size: 14px;
  font-family: Helvetica, "Helvetica Neue", "Century Gothic", "微软雅黑", "Microsoft Yahei", "STXihei";
}

.navbar .navbar-nav a {
  font-weight: 100;
  text-transform: uppercase;
  -webkit-transition: color 0.15s;
  -o-transition: color 0.15s;
  transition: color 0.15s;
}

#content{
	height: 622px;
	/*background: #3083eb;*/
	margin: 0px;
	position: relative;
}

.login-panel{
	position: absolute;
	background: #70aae5;
    -webkit-box-shadow: 0 0 2px rgba(0,0,0,.13),0 1px 5px rgba(0,0,0,.36),inset 0 0 0 1px rgba(255,255,255,0.2);
    -moz-box-shadow: 0 0 2px rgba(0,0,0,.13),0 1px 5px rgba(0,0,0,.36),inset 0 0 0 1px rgba(255,255,255,0.2);
    box-shadow: 0 0 2px rgba(0,0,0,.13),0 1px 5px rgba(0,0,0,.36),inset 0 0 0 1px rgba(255,255,255,0.2);
    height: 356px; 
    right: 0px;
    top: 100px;
    /*top:100%;*/
    width: 388px;
    border-radius: 10px;
}

.login-panel h1{
	color: #fff;
    font-weight: 400;
    font: normal 20px/75px "Microsoft Yahei","微软雅黑",SimSun,sans-serif;
    height: 66px;
    margin-left: 30px;
    width: 227px;
}

.login-panel .container{
	padding-left: 30px;
    top: 45px;
    width: 380px;
}

.input-group{
	width: 320px;
}

input.form-control {
	float: left;
    height: 48px;
    font-size: 18px;
    box-shadow: none;
    font-family: Helvetica, "Helvetica Neue", "Century Gothic", "微软雅黑", "Microsoft Yahei", "STXihei";
}

.submit{
	width: 320px !important;
	height: 50px;
	background-color:#4488f0;
	color:white;
	text-shadow:1px 1px 1px #555555;
	/*border:1px solid #fe970f;*/
	cursor:pointer;
	-webkit-appearance:none;
	font-family: Helvetica, "Helvetica Neue", "Century Gothic", "微软雅黑", "Microsoft Yahei", "STXihei";
}

.button {
    display: block;
    border-radius: 3px;
    background: #ff7b33;
    color: white;
    font-size: 18px;
    font-weight: normal;
    height: 50px;
    line-height: 50px;
    padding: 0;
    position: relative;
    text-align: center;
}

.text-tagline{
	margin: 0 0 4.5px;
}

.feature-header{
	margin-top: 100px;
    font-size: 26px;
    font-weight: bold;
    color: #F5FFFA;
}

.text-lead{
	font-size: 18px;
	color: #F5FFFA;
}

@media (min-width: 992px) {
  .feature-block {
    padding-bottom: 150px;
    padding-top: 150px;
  }
  .feature-header {
    font-size: 48px;
  }
}

.site-footer {
  color: #999;
}

.site-footer .site-footer-copyright{
	width: 100%;
	text-align: center;
	position: absolute;
	bottom: 100px;
}

.site-footer a{
	color: #999;
}
