<style type="text/css">
	.box-wrap{
		position: absolute;
		width: 100%;
		top: 50px;
		bottom: 1px;
		overflow: auto;
	}
</style>
<div class="mod-search cf">
	<div class="fl">
		<ul class="ul-inline">
			<li>
				<label></label>
			</li>
			<li>
	        </li>
		</ul>
	</div>
	<div class="fr">
		<a id="btn-add-project" href="javascript:;" class="ui-btn ui-btn-sp">新建项目</a>
	</div>
</div>
<div class="box-wrap wrapper-content">
	<div class="project-list">
		
	</div>
</div>
<script id="projectTemp" type="text/html">
	<table class="table table-hover">
		<tbody>
			{{each list as value index}}
			<tr id="table_tr_{{value.id}}">
				<td class="project-status">
                    <span class="label label-info">进行中</span>
                </td>
                <td class="project-title">
                    <a href="javascript:;" onclick="THISPAGE.goMain();">{{value.title}}</a>
                    <br/>
                    <small><label>时间 </label> {{value.beginDate}}至{{value.endDate}}</small>&nbsp;
                    <small><label>任务</label> {{value.renwu}}个</small>
                </td>
                <td class="project-completion">
                    <small>当前进度： {{value.schedule}}%</small>
                    <div class="progress progress-mini">
                        <div style="width: {{value.schedule}}%;" class="progress-bar"></div>
                    </div>
                </td>
                <td class="project-people">
                    <a class="user" href="javascript:;"><img alt="image" class="img-circle" src="../../../img/avatar.jpg"></a>
                    <small>{{value.change}}</small>
                </td>
                <td class="project-actions">
                	<div class="project-btn-group">
                		<a href="javascript:;" p-id="{{value.id}}" type="list" class="btn btn-white btn-circle"><i class="fa fa-info"></i></a>
                        <a href="javascript:;" p-id="{{value.id}}" type="edit" class="btn btn-white btn-circle"><i class="fa fa-pencil"></i></a>
                        <a href="javascript:;" p-id="{{value.id}}" type="lock" class="btn btn-white btn-circle"><i class="fa fa-lock"></i></a>
                        <a href="javascript:;" p-id="{{value.id}}" type="del" class="btn btn-white btn-circle"><i class="fa fa-close"></i></a>
                	</div>
                </td>
			</tr>
			{{/each}}
		</tbody>
	</table>
</script>
<script type="text/javascript" src="../../../js/app/oa/project/temp_project_list.js" ></script>