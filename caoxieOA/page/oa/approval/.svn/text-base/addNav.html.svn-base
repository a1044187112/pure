    },{
    	no: 4,
    	url: 'evection/add.html',
    	title: '出差申请单',
    	remark: '暂无审批表描述'
    },{
    	no: 5,
    	url: 'out/add.html',
    	title: '外出申请单',
    	remark: '暂无审批表描述'
    },{
    	no: 6,
    	url: 'leave/add.html',
    	title: '请假申请单',
    	remark: '暂无审批表描述'
    },{
    	no: 7,
    	url: 'sendcar/add.html',
    	title: '派车申请单',
    	remark: '暂无审批表描述'
    }];
    var colors = ['#FFEC8B','#FFB90F','#FF7F24','#87CEFF','#63B8FF','#66CD00','#00EEEE'];
    var NavLIST =  template.compile('<li class="js_formitemsli" sort="6" url="{{url}}">'+
					'<div class="list-box js_formListBox">'+
						'<div class="content">'+
							'<span id="form-number" class="serial" style="background-color: {{color}};">{{no}}</span>'+
							'<h4 id="form-title" class="title router ellipsis filterSlip_js js_formtitle"  title="{{title}}">{{title}}</h4>'+
							'<p id="form-describe" class="ellipsis nodescription" title="{{remark}}">{{remark}}</p>'+
						'</div>'+
					'</div>'+
				'</li>');
	$(function(){
		var navDom = "";
		$.each(navListData,function(i,item){
			navDom += NavLIST({
				no: i,
				color: !colors[i] ? '#00EEEE' : colors[i],
				url: item.url,
				title: item.title,
				remark: item.remark
			});
		})
		$('.wform-list').html(navDom);
		$('.wform-list li').on('click',function(){
			var url = $(this).attr('url');
			THISPAGE.sidePanel.options.closeBtn = 0;
			THISPAGE.sidePanel.realod(url);
		});
	});
	
</script>