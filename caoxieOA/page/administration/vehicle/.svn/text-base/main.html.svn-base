	<body>
		<!-- siled begin -->
		<div class="sidebard-panel">
			<div class="header" style="text-align: center;">
				<a href="javascript:;" class="ui-btn ui-btn-sp" id="add-main"><i class="fa fa-plus"></i>添加新车辆</a>
			</div>
			<div class="sidebar-content p5">
				<ul id="nav-btn" class="left-selector">
